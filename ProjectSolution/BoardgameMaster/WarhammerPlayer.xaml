<UserControl x:Class="BoardgameMaster.WarhammerPlayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BoardgameMaster"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="160">
	<Grid>
		<TextBox Name="Num" Width="50" Height="50" Margin="55,20,55,80" />
		<Button Name="Generate" Height="30" Width="100" Margin="30,85,30,35" Click="Generate_Click"/>
		<Button Name="Delete" Height="30" Width="30" Canvas.Right="10" Click="Delete_Click" Canvas.Left="220" Margin="125,5,5,115"/>
	</Grid>
</UserControl>
